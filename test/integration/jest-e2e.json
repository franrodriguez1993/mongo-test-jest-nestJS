{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@configs/(.*)$": "<rootDir>/src/configs/$1",
    "^@database/(.*)$": "<rootDir>/src/database/$1",
    "^@modules/(.*)$": "<rootDir>/src/App/modules/$1"
  }
  
}
